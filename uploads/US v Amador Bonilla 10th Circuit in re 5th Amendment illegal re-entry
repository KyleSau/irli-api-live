<script type="text/javascript">
function validateForm() {
	f1 = document.forms[0];
	if( ! CheckCaseNum(f1.csnum1.value,true,false,f1.csnum1) ){return 0;}
	if( ! CheckCaseNum(f1.csnum2.value,true,false,f1.csnum2) ){return 0;}
	if( ! CompareCaseNum(f1.csnum1.value,f1.csnum2.value,null,f1.csnum1) ){
		return 0;
	}
	// remove leading and trailing whitespace (back end also does this)
	if (f1.aName.value.length > 0)
		f1.aName.value = f1.aName.value.replace(/^\s*/g,"").replace(/\s*$/g,"");

	if (false && f1.aName.value.length > 0 && !f1.searchPty.checked &&
		!f1.searchAty.checked) {	// false is debug ONLY!!
			alert("A name search requires the Party or Attorney checkbox " +
				"to be checked!");
		return 0;
	}
	//  we have a name              it has less than two non-wild characters
	if (f1.aName.value.length > 0 && !f1.exactMatches.checked &&
		f1.aName.value.replace(/[,\s\*\?]/g,"").length < 2 ) {
			alert("A name search should have at least 2 " +
				"non-wildcard characters!");
			return 0;
	}
	//issue 25164
	if (f1.exactMatches.checked) {
		for (i =0; i <= f1.aName.value.length -1; i++) {
			if (f1.aName.value.charAt(i) == "*" || f1.aName.value.charAt(i) == "?") {
				alert("Wildcard search not allowed when 'Exact Matches Only' is checked.");
				return 0;
			}
		}
	}


	f1.submit();
}
</script>
